<p id="notice"><%= notice %></p>

<h1>Listing Dominant Foots</h1>

<table>
  <thead>
    <tr>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dominant_foots.each do |dominant_foot| %>
      <tr>
        <td><%= dominant_foot.description %></td>
        <td><%= link_to 'Show', dominant_foot %></td>
        <td><%= link_to 'Edit', edit_dominant_foot_path(dominant_foot) %></td>
        <td><%= link_to 'Destroy', dominant_foot, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Dominant foot', new_dominant_foot_path %>
